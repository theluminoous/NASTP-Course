    "from PIL import Image\n",
    "link = input(\"Enter the link to generate a QR code : \")\n",
    "qr_code = pyqrcode.create(link)\n",
    "qr_code.png(\"QRcode.png\", scale = 5)\n",
    "Image.open(\"QRcode.png\")"
